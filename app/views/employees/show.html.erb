<h1>Employee:</h1>
<h2>
  <p>Name: <%=@employee.full_name%>
  <p>Alias: <%=@employee.alias%>
  <p>Title: <%=@employee.title%>
  <p>Office: <%=@employee.office%>
  <p>Picture: <%=image_tag "#{@employee.img_url}"%>
  <p>Dog: <%=link_to @employee.dog.name, dog_path(@employee.dog.id)%>
    <h3> <%= button_to 'Edit Employee', edit_employee_path(@employee), :method => :get %></h3>
</h2>
